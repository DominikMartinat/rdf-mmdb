  <owl:NamedIndividual rdf:about="https://rdf.molmedb.upol.cz/vocabulary#KcalMol">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://rdf.molmedb.upol.cz/vocabulary#KcalMol"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/UO_0000111"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">kcal/mol</rdfs:label>
        <obo:IAO_0000115>A derived unit of molar thermodynamic energy defined as the energy equal to 1000 calories in one mole of substance.</obo:IAO_0000115>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="https://rdf.molmedb.upol.cz/vocabulary#CmS">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://rdf.molmedb.upol.cz/vocabulary#CmS"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/UO_0000060"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cm/s</rdfs:label>
        <obo:IAO_0000115>Centimeter per second. A derived unit of speed/velocity equivalent to 0.01 meter of distance traveled per 1 second.</obo:IAO_0000115>
  </owl:NamedIndividual>



</rdf:RDF>