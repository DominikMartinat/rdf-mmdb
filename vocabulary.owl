<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:obo="http://purl.obolibrary.org/obo/"
         xmlns:owl="http://www.w3.org/2002/07/owl#">
  
  
  <!--
   ////////////////////////////////////
   // TRANSPORTER ENDPOINT CLASSES
   ////////////////////////////////////
    -->

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#PKm">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0000477"/>
    <obo:IAO_0000115>pKm is -log Km, where Km is the concentration of substrate at ½ Vmax, according to the Henri-Michaelis-Menten kinetic model.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">pKm</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#PKi">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0000192"/>
    <obo:IAO_0000115>pKi is -log Ki, where Kmi is  the equilibrium inhibitor dissociation constant, the concentration of the competing inhibitor that results in binding to half the enzymes sites at equilibrium in the absence of substrate or other competitors.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">pKi</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#InhibitionAssay">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0080024"/>
    <obo:IAO_0000115>Result of the assay whether the measured entity acts as inhibitor of the target enzyme.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">inhibition assay</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#SubstrateBindingAssay">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0080024"/>
    <obo:IAO_0000115>Result of the assay whether the measured entity acts as substrate of the target enzyme.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">substrate assay</rdfs:label>
  </owl:Class>

 <!--
   ////////////////////////////////////
   // INTERACTION ENDPOINT CLASSES
   ////////////////////////////////////
   -->
  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#LogK">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002128"/>
    <obo:IAO_0000115>Logarithm of partition coefficient membrane/water.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">LogK</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#LogPerm">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002808"/>
    <obo:IAO_0000115>Logarithm of membrane permeability.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">LogPerm</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#PositionOfMinima">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002118"/>
    <!--graphical calculation endpoint - values obtained from curve? --> 
    <obo:IAO_0000115>Position of minima along the membrane normal from the membrane center.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">position of minima</rdfs:label>
  </owl:Class> 

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#PenetrationBarrier">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002118"/>
    <obo:IAO_0000115>Penetration barrier (maximum - minimum).</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">penetration barrier</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#DepthOfMinima">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002118"/>
    <obo:IAO_0000115>A depth of minima within membrane.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">depth of minima</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#ContactAngle">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0080028"/>
    <!--perhaps different endpoint superclass, maybe just endpoint BAO_0000179-->
    <obo:IAO_0000115>Contact angle in membrane.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">contact angle</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#AbsorbtionWavelength">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002118"/>
    <obo:IAO_0000115>Peak absorbtion wavelength.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">peak absrobtion wavelength</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#FluorescenceWavelength">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002118"/>
    <obo:IAO_0000115>Peak fluorescence wavelength.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">peak fluorescence wavelength</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#QuantumYield">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0000179"/>
    <!--just subclass of endpoint, not sure about closer classification-->
    <obo:IAO_0000115>Number of times a specific event occurs per photon absorbed by the system.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">quantum yield</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#FluorescenceLifetime">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002114"/>
    <obo:IAO_0000115>The time a fluorophore spends in the excited state before emitting a photon and returning to the ground state.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">fluorescence lifetime</rdfs:label>
  </owl:Class>

   <!--
   ////////////////////////////////////
   // Membrane CLASSES - temporary solution
   ////////////////////////////////////
   --> 

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#MembraneModel">
    <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0003114"/>
    <!-- should be subclass of BAO_0003111 to comply with BAO_0000015 restrictions but it is not macromolecule -->
    <obo:IAO_0000115>Model of biological membrane used in experimental and computational assays of interactions of molecular entities with membranes. Contains biological membranes, lipid membrane models, nonlipid membrane models (eg. octanol) etc.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">membrane model</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#MembraneModelByLocation">
    <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#MembraneModel"/>

    <obo:IAO_0000115>Cathegory of membrane model according to where are approximated membranes located anatomically. It can be bound to tissue or cells.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">membrane model by location</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#BrainMembraneModel">
    <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#MembraneModelByLocation"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">brain membrane model</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#CellMembraneModel">
    <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#MembraneModelByLocation"/>
    <obo:IAO_0000115>Model of a membrane located in brain tissue.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cell membrane model</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#EyeMembraneModel">
    <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#MembraneModelByLocation"/>
    <obo:IAO_0000115>Model of a membrane located in eye tissue.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">eye membrane model</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#GenericMembraneModel">
    <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#MembraneModelByLocation"/>
    <obo:IAO_0000115>Model of a membrane without specified anatomical location.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">generic membrane model</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#IntestineMembraneModel">
    <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#MembraneModelByLocation"/>
    <obo:IAO_0000115>Model of a membrane located in intestine tissue.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">intestine membrane model</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#OralMembraneModel">
    <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#MembraneModelByLocation"/>
    <obo:IAO_0000115>Model of a membrane located in mouth tissue.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">oral membrane model</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#SkinMembraneModel">
    <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#MembraneModelByLocation"/>
    <obo:IAO_0000115>Model of a membrane located in skin tissue.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">skin membrane model</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#CellBasedMembraneModel">
    <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#MembraneModel"/>
    <obo:IAO_0000115>Model of a membrane consisting of cells.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">skin membrane model</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#SubstanceBasedMembraneModel">
    <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#MembraneModel"/>
    <obo:IAO_0000115>Model of a membrane consisting one or more chemical substances.</obo:IAO_0000115>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">skin membrane model</rdfs:label>
  </owl:Class>

   <!--
   ////////////////////////////////////
   //Method INDIVIDUALS/CLASSES - temporary solution
   ////////////////////////////////////
   --> 
  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#SimulationByResolution">
      <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002282"/>
      <obo:IAO_0000115>Granularity level at which is simulated system modelled (e.g. atomistic, coarse grained etc.).</obo:IAO_0000115>
      <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">computer simulation method by resolution</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#AtomisticSimulation">
      <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#SimulationByResolution"/>
      <obo:IAO_0000115>Simulation where every atom of simulated system is explicitly represented.</obo:IAO_0000115>
      <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">atomistic simulation</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#UnitedAtomsSimulation">
      <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#SimulationByResolution"/>
      <obo:IAO_0000115>Simulation where every atom of simulated system, except hydrogen, is represented.</obo:IAO_0000115>
      <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">united atoms simulation</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#CoarseGrainedSimulation">
      <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#SimulationByResolution"/>
      <obo:IAO_0000115>Simulation where groups of multiple atoms of simulated system are represented as one entity.</obo:IAO_0000115>
      <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">coarse grained simulation</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#HybridResolutionSimulation">
      <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#SimulationByResolution"/>
      <obo:IAO_0000115>Simulationusing multiple levels of granularity for representation of atoms in a simulated system.</obo:IAO_0000115>
      <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hybrid resolution simulation</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#MembraneInteractionAssessmentMethod">
      <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002202"/>
      <obo:IAO_0000115>Experimental method for quantification of interaction between molecule and membrane (e.g. peremability, partition etc.).</obo:IAO_0000115>
      <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">membrane interaction assessment method</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#WaterMembranePartitioningMethod">
      <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#MembraneInteractionAssessmentMethod"/>
      <obo:IAO_0000115>Method measuring partitioning of molecules between water and membrane.</obo:IAO_0000115>
      <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">water/membrane partition measuring method</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#MembranePositionMethod">
      <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#MembraneInteractionAssessmentMethod"/>
      <obo:IAO_0000115>Method measuring distance of stable position of molecule from the centre of a membrane.</obo:IAO_0000115>
      <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">position measuring method</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://rdf.molmedb.upol.cz/vocabulary#MembranePermeabilityMethod">
      <rdfs:subClassOf rdf:resource="https://rdf.molmedb.upol.cz/vocabulary#MembraneInteractionAssessmentMethod"/>
      <obo:IAO_0000115>Method measuring permeability of a molecule across a membrane.</obo:IAO_0000115>
      <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">membrane peremability measuring method</rdfs:label>
  </owl:Class>  


