@prefix cito: <http://purl.org/spar/cito/> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .

#publications - not databases
#databases have to be separated manually from other references now. That is not optimal.
#database tripples are made manualy in different file.

<#TriplesMap1>
  rr:logicalTable [ rr:sqlQuerry """
    SELECT id, doi, citation, pmid FROM publications WHERE citation NOT IN ('in-house calculations','PUBCHEM', 'ChEMBL', 'IUPHAR', 'Metrabase')
    """ ] ;
  rr:subjectMap [
    rr:template "http://rdf.molmedb.upol.cz/reference/ref{id}" ;
    rr:class dcterms:BibliographicResource
  ] ;
    rr:predicateObjectMap [
    rr:predicate dcterms:bibliographicCitation ;
    rr:objectMap [ rr:column "citation" ]
  ] ;
    rr:predicateObjectMap [
    rr:predicate bibo:doi ;
    rr:objectMap [ rr:column "doi"]
  ] ;
    rr:predicateObjectMap [
    rr:predicate bibo:pmid ;
    rr:objectMap [ rr:column "pmid" ]
  ] .
#which interactions and substances there in source - database included
<#TriplesMap2>
  rr:logicalTable [ rr:sqlQuerry """
    SELECT id, id_reference,
      (CASE WHEN LENGTH(id_substance)<5 THEN LPAD(id_substance,5,0) ELSE id_substance END) padded_id_substance FROM interaction
  """] ;
  rr:subjectMap [
    rr:template "http://rdf.molmedb.upol.cz/reference/ref{id_reference}"
  ] ;
    rr:predicateObjectMap [
    rr:predicate cito:isCitedAsDataSourceBy ;
    rr:objectMap [ rr:template "http://rdf.molmedb.upol.cz/interaction/int{id}" ]
  ] ;
    rr:predicateObjectMap [
    rr:predicate dcterms:subject ;
    rr:objectMap [ rr:template "http://identifiers.org/molmedb/MM{padded_id_substance}" ]
  ] .

#which transproter measurements and substances there in source - database included                  
<#TriplesMap3>
  rr:logicalTable [ rr:sqlQuerry """
    SELECT id, id_reference,
      (CASE WHEN LENGTH(id_substance)<5 THEN LPAD(id_substance,5,0) ELSE id_substance END) padded_id_substance FROM transporters
  """] ;
  rr:subjectMap [
    rr:template "http://rdf.molmedb.upol.cz/reference/ref{id_reference}"
  ] ;
    rr:predicateObjectMap [
    rr:predicate cito:isCitedAsDataSourceBy ;
    rr:objectMap [ rr:template "http://rdf.molmedb.upol.cz/transporter/tra{id}"]
  ] ;
    rr:predicateObjectMap [
    rr:predicate dcterms:subject ;
    rr:objectMap [ rr:template "http://identifiers.org/molmedb/MM{padded_id_substance}" ]
  ] .