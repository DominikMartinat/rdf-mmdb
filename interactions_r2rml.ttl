<#TriplesMap1>
  rr:logicalTable [ rr:sqlQuerry """
    SELECT id,
      (LPAD(id,6,0)) padded_id,
      (CASE WHEN LENGTH(id_substance)<5 THEN LPAD(id,5,0) ELSE id_substance END) padded_id_substance,
      (LPAD(id_membrane,6,0) padded_id_membrane,
      (LPAD(id_reference,6,0) padded_id_reference from interactions
  """];
  rr:subjectMap [
    rr:template "http://rdf.molmedb.upol.cz/interaction/{padded_id}";
    rr:class bao:BAO_0000015;
  ];
    rr:predicateObjectMap [
    rr:predicate bao:BAO_0090012;
    rr:objectMap [ rr:template "http://identifiers.org/molmedb/MM{padded_id_substance}" ]
  ];
    rr:predicateObjectMap [
    rr:predicate bao:BAO_0090012;
    rr:objectMap [ rr:template "http://rdf.molmedb.upol.cz/interaction/membrane_{padded_id_membrane}"  ]
  ];
    rr:predicateObjectMap [
    rr:predicate bao:BAO_0000209;
    rr:objectMap [ rr:template "http://rdf.molmedb.upol.cz/interaction/measure_group_{padded_id}"  ]
  ];
    rr:predicateObjectMap [
    rr:predicate bao:BAO_0000205;
    rr:objectMap [ rr:constant bao:BAO_0000100 ]
  ];
    rr:predicateObjectMap [
    rr:predicate cito:citesAsDataSource;
    rr:objectMap [ rr:template "http://rdf.molmedb.upol.cz/reference/{padded_id_reference}"  ]
  ].

<#TriplesMap2>
  rr:logicalTable [ rr:sqlQuerry """
    SELECT id, comment,
      (LPAD(id,6,0)) padded_id,
      (CASE WHEN LENGTH(id_substance)<5 THEN LPAD(id,5,0) ELSE id_substance END) padded_id_substance,
      (LPAD(id_membrane,6,0)) id_membrane,
      (LPAD(id_method,6,0)) id_method,
      (CASE WHEN LogK IS NULL THEN NULL ELSE LPAD(id,6,0) END) LogK_id,
      (CASE WHEN LogPerm IS NULL THEN NULL ELSE LPAD(id,6,0) END) LogPerm_id,
      (CASE WHEN temperature IS NULL THEN NULL ELSE LPAD(id,6,0) END) temperature_id,
      (CASE WHEN charge IS NULL THEN NULL ELSE LPAD(id,6,0) END) charge_id FROM interaction
  """];
  rr:subjectMap [
    rr:template "http://rdf.molmedb.upol.cz/interaction/measure_group_{padded_id}";
    rr:class bao:BAO_0000040;
  ];
    rr:predicateObjectMap [
    rr:predicate bao:BAO_0000212;
    rr:objectMap [ rr:template "http://rdf.molmedb.upol.cz/interaction/method_{padded_id_method}" ]
  ];  
    rr:predicateObjectMap [
    rr:predicate bao:BAO_0000208;
    rr:objectMap [ rr:template "http://rdf.molmedb.upol.cz/interaction/measure_group_logk_{LogK_id}" ]
  ]; 
    rr:predicateObjectMap [
    rr:predicate bao:BAO_0000208;
    rr:objectMap [ rr:template "http://rdf.molmedb.upol.cz/interaction/measure_group_logperm_{LogPerm_id}" ]
  ]; 
    rr:predicateObjectMap [
    rr:predicate bao:BAO_0090012;
    rr:objectMap [ rr:template "http://identifiers.org/molmedb/MM{padded_id_substance}" ]
  ];
    rr:predicateObjectMap [
    rr:predicate bao:BAO_0090012;
    rr:objectMap [ rr:template "http://rdf.molmedb.upol.cz/interaction/membrane_{padded_id_membrane}"  ]
  ];
    rr:predicateObjectMap [
    rr:predicate bao:BAO_0000426;
    rr:objectMap [ rr:template "http://rdf.molmedb.upol.cz/interaction/{padded_id}" ]
  ]; 
    rr:predicateObjectMap [
    rr:predicate repr:hasExperimentalCondition;
    rr:objectMap [ rr:template "http://rdf.molmedb.upol.cz/interaction/measure_group_temperature_{temperature_id}" ]
  ]; 
    rr:predicateObjectMap [
    rr:predicate repr:hasExperimentalCondition;
    rr:objectMap [ rr:template "http://rdf.molmedb.upol.cz/interaction/measure_group_mol_charge_{charge_id}" ]
  ]; 
    rr:predicateObjectMap [
    rr:predicate rdfs:comment;
    rr:objectMap [ rr:column "comment" ]
  ].

